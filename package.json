		"@sveltejs/kit": "^1.20.4",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.5",
		"vite": "^4.4.10",
		"vitest": "^0.32.2"
	},
	"type": "module",
	"dependencies": {
		"bcrypt": "^5.1.1",
		"bcryptjs": "^2.4.3",
		"gh-pages": "^6.0.0",
		"lodash.debounce": "^4.0.8"
